<script >
		function yscroll(){
			var pagetop = document.getElementById("land-head");
			var ypos = window.pageYOffset;
			if(ypos > 100){
				pagetop.style.display = "inherit";
				
			}
			else{
				pagetop.style.display = "none";
			}
		}

		window.addEventListener("scroll", yscroll);
</script>
