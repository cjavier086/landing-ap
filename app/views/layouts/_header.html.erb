
<header class="navbar navbar-fixed-top navbar-inverse navbar-nav" id="land-head" style="background-color:transparent;display:none;">
  <div class="container">
    <nav>
      <!-- #ebebe0-->
      <ul class="nav navbar-nav navbar-right">
      <li class="li-head" style="margin-top:-4px;">
        <%= link_to image_tag("logo.png", alt: "Imagen del Curso", size:"120x24"), "#video"%>
      </li>
       <li class="li-head" style="top:4px;" id="what">
          <%= link_to "¿Qué es Movivan?", "#about", :onclick => "document.getElementById('login').style.backgroundColor = '#ff0066'; document.getElementById('sign').style.backgroundColor = '#ff0066'; document.getElementById('cont').style.borderRight = 'solid 1px #ff0066'" %>
